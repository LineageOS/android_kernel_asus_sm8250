// ASUS_BSP +++ Jiunhau_Wang [ZS661KS][Touch][RD][Porting] touch mini-porting
 &qupv3_se13_i2c {
	status = "ok";
	normandy-i2c@5d{
		compatible = "goodix,gt9886";
