&pm8150b_gpios {
	gpio12_rst_pin {
		gpio12_rst_on: gpio12_rst_on {
			pins = "gpio12"; /* GPIO 12 */
			function = "normal"; /* normal output */
			power-source = <0>; /* VIN0 */
			output-high; /* digital output, no invert */
		};
		
		gpio12_rst_off: gpio12_rst_off {
			pins = "gpio12"; 
			function = "normal"; 
			bias-disable;
			output-low; // digital output, no invert 
			power-source = <0>; 
		};
	};
};


